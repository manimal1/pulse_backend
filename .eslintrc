{
  "env": {
    "node": true,
    "browser": false
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": false,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true
  },
  "rules": {
    "strict": 0,
    "quotes": [2, "single"],
    "comma-dangle": [1, "never"],
    "no-cond-assign": [2, "always"],
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [2, "functions"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 1,
    "no-unexpected-multiline": 2,
    "accessor-pairs": 1,
    "block-scoped-var": 2,
    "curly": [2, "all"],
    "default-case": 2,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": "^[a-z]+(_[a-z]+)+$"}],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 1,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2, {"number": true}],
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-with": 2,
    "radix": 2,
    "yoda": 2,


    "no-delete-var": 2,
    "no-shadow-restricted-names": 2,
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "after-used"
    }],
    "no-use-before-define": 2,


    "array-bracket-spacing": [2, "never"],
    "brace-style": [2,
      "1tbs", {
        "allowSingleLine": false
      }
    ],
    "camelcase": [2, {
      "properties": "always"
    }],
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }],
    "comma-style": 2,
    "computed-property-spacing": [2, "never"],
    "consistent-this": [1, "self"],
    "indent": [2, 2],
    "lines-around-comment": [2, {
      "beforeBlockComment": true,
      "beforeLineComment": false
    }],
    "new-cap": [2, {
      "newIsCap": true
    }],
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": [2, {
      "skipBlankLines": true
    }],
    "object-curly-spacing": [2, "never"],
    "one-var": [0, "never"],
    "operator-linebreak": [2, "after"],
    "quote-props": [2, "as-needed"],
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "semi": [2, "always"],
    "space-after-keywords": 2,
    "space-before-blocks": 2,
    "space-in-parens": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-return-throw-case": 2,
    "wrap-regex": 2,
    "arrow-parens": [2, "always"],
    "arrow-spacing": [2, {
      "before": true,
      "after": true
    }],
    "generator-star-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-class-assign": 2,
    "no-const-assign": 2
  }
}
